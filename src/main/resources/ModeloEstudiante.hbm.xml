<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="models.ModeloEstudiante" table="estudiante" schema="public" catalog="libreria">
        <id name="codigounico">
            <column name="codigounico" sql-type="integer"/>
        </id>
        <property name="contraseña">
            <column name="contraseña" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>